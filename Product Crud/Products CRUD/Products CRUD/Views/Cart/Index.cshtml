@model List<Products_CRUD.Models.Entity.Product>
@{
    ViewBag.Title = "Cart";
}

<h2>My Cart</h2>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>Description</th>
        
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.id</td>
            <td>@s.name</td>
            <td>@s.price</td>
            <td>@s.quantity</td>
            <td>@s.description</td>
           
        </tr>
    }
</table>

